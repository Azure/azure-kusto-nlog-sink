<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.nlog-project.org/schemas/NLog.xsd NLog.xsd"
      autoReload="true"
      throwExceptions="true"
      internalLogLevel="Off" internalLogFile="c:\temp\nlog-internal.log">
	<extensions>
		<add assembly="NLog.Azure.Kusto"/>
	</extensions>
	<targets>
		<!--  ADX target -->
		<target name="adxtarget"
				xsi:type="ADXTarget"
		ConnectionString="Data Source=https://somecluster.westeurope.dev.kusto.windows.net;Database=e2e;Fed=True;AppClientId=APP-ID;AppKey=APP-KEY;Authority Id=TENANT-ID"
		Database="e2e"
		TableName="ADXNlog"
		UseStreamingIngestion="false"
		FlushImmediately="true"
		/>
	</targets>
	<rules>
		<logger minlevel="Info" name="*" writeTo="adxtarget"/>
	</rules>
</nlog>